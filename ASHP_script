       VIMP_plot ,
       width=14,height=8)


Rsqu <- function(y, yhat) {
  mdata <- mean(y)
  1 - sum((y-yhat)^2)/(sum((y-mdata)^2))
}




#########################
####SIM data
sim_data <- data_on %>% select(Date,COP)
mysamp <- function(n, m, s, lwr, upr, rounding) {
  samp <- round(rnorm(n, m, s), rounding)
  samp[samp < lwr] <- lwr
  samp[samp > upr] <- upr
  samp
}
summary(data_on$ASHP_on)
#Min. 1st Qu.  Median    Mean 3rd Qu.    Max. 
#1      58      60      53      60      60 
sd(data_on$ASHP_on)
#14.44347
