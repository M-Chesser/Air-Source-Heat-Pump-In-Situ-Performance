data$steady_state[data$OutsideTemperature >= 21 & data$FlowTemperature >= 55 & data$EnergyConsumed >= 27 & data$on == 1] <- 1 


#Created this variable, the not in steady state to double check that the code above is correct.
data$steady_state_not <- 0
data$steady_state_not[data$on == 1 & data$steady_state == 0] <- 1 

data$check <- data$steady_state + data$steady_state_not
#If check = 2 there would have been a issue but there was none so dropped data$check.
data <- data[,c(1:11)]


#The missing datetimes (2,167 minutes) are added to the dataset and the values for each variable is NA.   
data_full_dates <- seq.POSIXt(min(data$TimeStamp), max(data$TimeStamp), by = "min")
data_full_dates <- data.frame(Date = data_full_dates)
data_complete <- merge.data.frame(data_full_dates, data, by.x = "Date", by.y = "TimeStamp", all.x = TRUE)
data <- data_complete
rm(data_complete, data_full_dates)
statsNA(data$EnergyConsumed)


#COP is difficult to analyse at the minute level due to the nature of its operation
#Start up involve large energy consumption and little energy produced giving small COP.
#While ramp down is the opposite very large unrealistic COP. 
#Examining at the hour level solves this. 
data$Datetime2 <- droplevels(cut(data$Date, breaks = "hour"))

#Variables that are Averaged
#Park et al (2018) intensive variables such as temperature and flow rate should be averaged. 
data_avg <- aggregate(cbind(RoomTemperatureZone1, 
                            RoomTemperatureZone2, 
                            FlowTemperature, OutsideTemperature) ~ Datetime2, 
                      data=data, FUN=mean, na.action = na.pass)
#As mentioned there is missing observations in the dataset.
#The decision was made when aggregating that if an hour had a missing value that it was given a NA value (na.action = na.pass), 
#As this addresses an case of an hour only having several minutes of data and could result in over/under estimates 

#Variables that are summed
#Park et al (2018) extensive variables such as energy consumption should be summed. 
data_sum <- aggregate(cbind( 
  EnergyConsumed, 
  EnergyProduced,on,steady_state, steady_state_not) ~ Datetime2, 
  data=data, FUN=sum, na.action = na.pass)

data_2 <- merge(data_avg, data_sum)
rm(data_avg,data_sum)
statsNA(data_2$RoomTemperatureZone2)
#Of 8786 hours we have 38 hours of NAs.

#Patching the missing data
data_2$EnergyConsumed_ss  <- na_seasplit(data_2$EnergyConsumed, find_frequency=TRUE )
data_2$EnergyProduced_ss <- na_seasplit(data_2$EnergyProduced, find_frequency=TRUE )
data_2$RoomTemperatureZone1_ss <- na_seasplit(data_2$RoomTemperatureZone1, find_frequency=TRUE )
data_2$RoomTemperatureZone2_ss <- na_seasplit(data_2$RoomTemperatureZone2, find_frequency=TRUE )
data_2$FlowTemperature_ss <- na_seasplit(data_2$FlowTemperature, find_frequency=TRUE)
data_2$OutsideTemperature_ss <- na_seasplit(data_2$OutsideTemperature, find_frequency = TRUE)
data_2$on_ss <- na_seasplit(data_2$on, find_frequency=TRUE)
data_2$steady_state_ss <- na_seasplit(data_2$steady_state, find_frequency=TRUE)
data_2$steady_state_not_ss <- na_seasplit(data_2$steady_state_not, find_frequency=TRUE)

#Three rows steady state + not in steady state are greater than 60.
data_2[7381,18] <- 53.5
data_2[6589,18] <- 54.5
data_2[6588,18] <- 53.5

#Tiding up the dataset 
data_2$EnergyConsumed  <- as.numeric(data_2$EnergyConsumed_ss) 
data_2$EnergyProduced <- as.numeric(data_2$EnergyProduced_ss)
data_2$RoomTemperatureZone1 <- as.numeric(data_2$RoomTemperatureZone1_ss)
data_2$RoomTemperatureZone2 <- as.numeric(data_2$RoomTemperatureZone2_ss)
data_2$FlowTemperature <- as.numeric(data_2$FlowTemperature_ss)
data_2$on <- as.numeric(data_2$on_ss)
data_2$steady_state <- as.numeric(data_2$steady_state_ss)
data_2$steady_state_not <- as.numeric(data_2$steady_state_not_ss)
data_2$OutsideTemperature <- as.numeric(data_2$OutsideTemperature_ss)


data_2$Date <- strptime(as.character(data_2$Datetime2),format="%Y-%m-%d %H:%M:%S")
data_2$Date <- as.POSIXct(data_2$Date,tz = "UTC")
class(data_2$Date)
str(data_2)
data_2$COP <- data_2$EnergyProduced/data_2$EnergyConsumed
data_2 <- data_2[,c(20,21,2:5,8,9)]

#Filtering the data set to the heating season.
data_heating_season <- data_2 %>% filter(Date > "2019-09-30 00:00:00" & Date < "2020-04-13 01:00:00")

#Merging with local weatherstation data
setwd()

weatherstation <- read.table(file = "hly3723.csv", header = TRUE, sep = ",")
weatherstation$date<- dmy_hm(weatherstation$date)
weatherstation$date <- strptime(as.character(weatherstation$date),format="%Y-%m-%d %H:%M:%S")
weatherstation$date <- as.POSIXct(weatherstation$date,tz = "UTC")
weatherstation <- weatherstation %>% select(date,rain,		
                                            temp,       
                                            dewpt,	              
                                            vappr,		
                                            rhum,		
                                            msl,wetb,	
                                            wdsp) %>% filter(date > "2019-09-30 00:00:00" & date < "2020-04-13 01:00:00")
setwd()

data_heating_season <- cbind(data_heating_season, Rain = weatherstation$rain)
data_heating_season <- cbind(data_heating_season, dewpt = weatherstation$dewpt)
data_heating_season <- cbind(data_heating_season, vappr = weatherstation$vappr)
data_heating_season <- cbind(data_heating_season, rhum = weatherstation$rhum)
data_heating_season <- cbind(data_heating_season, msl	 = weatherstation$msl)
data_heating_season <- cbind(data_heating_season, wdsp	 = weatherstation$wdsp)
data_heating_season <- cbind(data_heating_season, wetb = weatherstation$wetb)
rm(weatherstation)

#Only interested when the HP is in a heat producing mode (ie on > 0) 
data_on <- data_heating_season %>% filter(data_heating_season$on >0 )

#Investigating COP values
COP <- data_on$COP
range(data_on$COP)
breaks = seq(0, 6, by=0.5)
COP.cut = cut(COP, breaks, right=FALSE)
COP.freq = table(COP.cut)
COP.freq
table(data_on$COP == 0)
#Even though the ASHP is on(a heat producing mode) 26 times COP is exactly zero
#It should be at least a near zero value.
#decision drop these rows
data_on<-data_on[!(data_on$COP == 0),]

#Checking small COP values against time on, 
#if ASHP is on for more than 10 minutes its reasonable to assume that COP should be greater than near zero. 
table(data_on$COP < 1, data_on$on > 14)
#COP of 0.03  after being on for an Hour and COP of .5 after being on for 56 minutes cop .5, 
#drop both these rows. 
data_on<-data_on[!(data_on$COP < 1 & data_on$on > 14),]

#viewed a boxplot of COP
boxplot(data_on$COP)
#group of outliers very close to zero values less than .5
#All on for less than 5 minutes.
#2019-10-22 18:00:00 
#start up end of hour and running into next hour 
#2020-02-29 16:00:00 
#start up end of hour and running into next hour 
#2019-10-03 23:00:00 
#start up end of hour and running into next hour 
#2020-02-20 21:00:00 
#start up end of hour and running into next hour 
#2019-11-06 23:00:00 
#middle of the hour for two minutes, drop this row. 
data_on<-data_on[!(data_on$Date == "2019-11-06 23:00:00"),]
#Group of outliers with values greater than .5 & less than 1
#2019-10-03 06:00:00 - start up end of hour and running into next hour 
#2019-10-09 09:00:00 - start up end of hour and running into next hour 
#2019-12-09 16:00:00 - start up end of hour and running into next hour 
#2020-03-23 12:00:00 - start up end of hour and running into next hour 
#2019-10-31 07:00:00 - start up end of hour and running into next hour 
#2020-01-11 10:00:00 - Shut down end of hour from running in last hour 
#2019-11-03 17:00:00 - start up end of hour and running into next hour
#2020-03-06 17:00:00 - start up end of hour and running into next hour
#2020-03-05 16:00:00 - start up end of hour and running into next hour
#2019-10-02 10:00:00 - start up end of hour and running into next hour
#Checking the flow temp
#flow temp for heating is 35 +
#so if flow temp is low after a long period of time there may be an issue with the data
#Flow temperature
table(data_on$on > 29, data_on$FlowTemperature <30)

#       FALSE TRUE
#FALSE   340   63
#TRUE   2998    8
low_flowtemp <- data_on %>% filter(data_on$on > 29, data_on$FlowTemperature <30)
#
#Date                  COP     Energyconsumed   EnergyProduced    FlowTemperature    on
#2020-03-18 03:00:00	3.101911	157	             487	           29.86667	         	60
#Checkec the average energy consumed for when ashp is on for an hour 
data_on %>% filter( data_on$on > 59) %>% summarise_at(c("EnergyConsumed"), mean)
#Average energy consumed when on for 60 minutes is 1,096, drop previous as there was probably an error at the minute level with the operating mode variable.
data_on<-data_on[!(data_on$Date == "2020-03-18 03:00:00"),]
rm(low_flowtemp)

#Reorganising the dataset. shorting names of variables
data_on <- data_on %>% select(Date,COP,FlowTemperature,on, 
                              steady_state, RoomTemperatureZone1,
                              RoomTemperatureZone2, Rain,OutsideTemperature, wetb,dewpt,vappr,rhum,msl,wdsp, wetb)
data_on <- data_on %>% 
rename(flowtemp = FlowTemperature)
data_on <- data_on %>% 
  rename(ASHP_on = on)
data_on <- data_on %>% 
  rename(outtemp = OutsideTemperature)
data_on <- data_on %>% 
  rename(zone1temp = RoomTemperatureZone1)
data_on <- data_on %>% 
  rename(zone2temp = RoomTemperatureZone2)


#
#Outlier detection LOF
library(DMwR2)
library(lattice)
library(grid)

LOF <- data_on  %>% select(COP,flowtemp,ASHP_on, 
                           steady_state, zone1temp,
                           zone2temp, Rain,outtemp, wetb,dewpt,vappr,rhum,msl,wdsp)
#Standardization ensures that features with large mean or 
#variance do not disproportionately influence the score
LOF <- scale(LOF)
outlier.scores <- lofactor(LOF, k=5)
plot(density(outlier.scores))
outliers <- order(outlier.scores, decreasing=T)[1:100]
print(outliers)
#2566,2785,2539, 83,1967,3317,1493,461,3238,1832,375,1833,1972,1984,3379,2453,985,2804,462,1116,2546,2786,1680,
#2830,936,2818,1881,2025,857,2459,2932,1229,1681,2065,3369,11,183,2424,,86,1679,3367,3226
#,2827,2414,2145,361,2850,2023,1799,53,3340,2574,2605,435,2555,2575,1835,1968,1834,3147,
#,867,3241,1239,2788,2168,188,1996,1992,960,2616,1973,2985,1228,2547,733,511,2837,2144,176,2635,2148,2782,2458,411
#,3373,1893,2478,1665,1497,,272,2317,1971,1781,994,309,2592,3276,2634,,120,1500
n <- nrow(LOF)
labels <- 1:n
labels[-outliers] <- "."
biplot(prcomp(LOF), cex=.8, xlabs=labels)
pch <- rep(".", n)
pch[outliers] <- "+"
col <- rep("black", n)
col[outliers] <- "red"
pairs(LOF, pch=pch, col=col)
data_on <- data_on[-c( 2566, 2785, 2539,   83, 1967, 3317, 1493,  461, 3238, 1832,  375, 1833, 1972, 1984, 3379, 2453,  
                       985, 2804,  462, 1116, 2546, 2786, 1680, 2830,  936, 2818, 1881, 2025,  857, 2459, 2932, 1229, 
                       1681, 2065, 3369,   11,  183, 2424,   86, 1679, 3367, 3226,
                       2827, 2414, 2145,  361, 2850, 2023, 1799,   53, 3340, 2574, 2605,  435, 2555, 2575, 1835, 1968, 
                       1834, 3147,  867, 3241, 1239, 2788, 2168,  188, 1996, 1992,  960, 2616, 1973, 2985, 1228, 2547,  
                       733,  511, 2837, 2144,  176, 2635, 2148, 2782, 2458,  411,
                       3373, 1893, 2478, 1665, 1497,  272, 2317, 1971, 1781,  994,  309, 2592, 3276, 2634,  120, 1500),]

