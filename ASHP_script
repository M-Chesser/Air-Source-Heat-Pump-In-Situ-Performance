rmse(test_data$COP,predict(gam_caret_7, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(gam_caret_7, test_data[,c(2:13)]))
GAM_residuals<-resid(gam_caret_7$finalModel)
png('gam_residuals.png',width = 8, height = 6, units = 'in', res = 300)
plot(train_data$COP,GAM_residuals, xlab="COP", ylab="GAM Residuals")
abline(0,0)
dev.off()
#Best model
set.seed(42)
gam_caret_10 <- train(
  COP ~ flowtemp + steady_state + outtemp +dewpt,
  data = train_data,
  method = "gam",
  trControl = mycontrol,
  importance = "impurity")
summary(gam_caret_10$finalModel)
gam_caret_10
set.seed(42)
gam.check(gam_caret_10$finalModel)
concurvity(gam_caret_10$finalModel)
concurvity(gam_caret_10$finalModel, full = FALSE)
rmse(test_data$COP,predict(gam_caret_10$finalModel, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(gam_caret_10, test_data[,c(2:13)]))
gamfinal <- gam_caret_10$finalModel
library(mgcViz)
library(devtools)
install_github("vr-vr/itsadug", force = TRUE)
library(itsadug)
png('gam_partial_effects.png',width = 8, height = 6, units = 'in', res = 350)
plot(gam_caret_10$finalModel, page = 1, shift = coef(gamfinal)[1], ylab = "COP")
dev.off()
png('vis.gam.png',width = 8, height = 6, units = 'in', res = 350)
vis.gam(gam_caret_10$finalModel, view = c(  "outtemp" ,  "flowtemp"), ylab= "flowtemp (C)",
        xlab = "outtemp (C)" ,cond=list( steady_state = 60 )  , zlab = "COP", ticktype = "detailed",  r = 5, 
        phi = 15,theta= 290,color="heat")
dev.off()

gam.check(gam)
gamfinal<- getViz(gam_caret_10$finalModel)
check.gamViz(gamfinal)
png('diagnostics_plots.png',width = 8, height = 6, units = 'in', res = 300)
set.seed(42)
check.gamViz(gamfinal,a.hist = list(bins = 10))
dev.off()




#RANDOM FOREST
####RF1
set.seed(42)
RF <- train(
  COP ~ flowtemp + ASHP_on + steady_state + zone1temp +
    zone2temp + outtemp + Rain + dewpt + rhum + msl +wdsp,
  data = train_data,
  method = "ranger",
  trControl = mycontrol,
  importance = "impurity",metric='RMSE')
summary(RF)
RF
#RMSE       R-SQ
#0.2823322  0.8355624
plot(RF)
print(RF)
rmse(test_data$COP,predict(RF, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(RF, test_data))
plot(varImp(RF))


###RF2
set.seed(42)
RF2 <- train(
  COP ~ flowtemp + outtemp+
    steady_state + zone1temp +
    + dewpt +Rain,
  data = train_data,
  method = "ranger",
  trControl = mycontrol,
  importance = "impurity",metric='RMSE')
summary(RF2 )
RF2
rmse(test_data$COP,predict(RF2, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(RF2, test_data))
plot(RF)
print(RF)
rmse(test_data$COP,predict(RF2, test_data[,c(3:13)]))
Rsqu(test_data$COP,predict(RF2, test_data))
