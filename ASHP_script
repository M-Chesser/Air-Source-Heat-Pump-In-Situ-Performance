qqcomp(list(fg, fl, fln), legendtext = plot.legend)
cdfcomp(list(fg, fl, fln), xlab = "COP",legendtext = plot.legend)
ppcomp(list(fg, fl, fln), legendtext = plot.legend)
gofstat(list(fg, fl, fln, fn),  fitnames = c("Gamma", "Logistic", "lognormal", "normal") )

