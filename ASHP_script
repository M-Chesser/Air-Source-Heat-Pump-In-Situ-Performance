N  <- 100000
set.seed(42)
df  <- data.frame(id = 1:N, flowtemp = mysamp(n=N, m=37.54, s=4.52278, lwr=24, upr=57, rounding=0),
                  outtemp =  mysamp(n=N, m=7.149, s=2.880651, lwr=0, upr=17.950, rounding=0),
                  ASHP_on = mysamp(n=N, m=53, s=14.44347, lwr=1, upr=60, rounding=1), 
                  steady_state = mysamp(n=N, m=47.91, s=14.56934, lwr=0, upr=60, rounding=1),
                  zone1temp = mysamp(n=N, m=21.16, s=0.7328685, lwr=18, upr=25, rounding=1),
                  zone2temp = mysamp(n=N, m=19.98, s=0.6152968, lwr=19, upr=24, rounding=1),
                  Rain = mysamp(n=N, m=0.1011, s=0.4264486, lwr=0, upr=5.5, rounding=1),
                  dewpt = mysamp(n=N, m=3.949, s=3.174836, lwr=-5.4, upr=14.100, rounding=1),
                  rhum = mysamp(n=N, m=86.21, s=8.813922, lwr=48, upr=100, rounding=1),
                  wdsp = mysamp(n=N, m=11.36, s=6.398625, lwr=1, upr=35, rounding=1),
                  msl = mysamp(n=N, m=1008.3, s=15.88484, lwr=971, upr=1048, rounding=1))
df$COP_RF2 <- predict(RF2, df)
df$COP_RF2 <- round(df$COP_RF2, 2)
df$COP_RF <- predict(RF, df)
df$COP_RF <- round(df$COP_RF, 2)
setwd()
save(df, file = "House_5_sim_data.RData")
load()

