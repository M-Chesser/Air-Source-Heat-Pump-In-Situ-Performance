summary(data_heating_season$COP)


##MODELS
Rsqu <- function(y, yhat) {
  
  mdata <- mean(y)
  
  1 - sum((y-yhat)^2)/(sum((y-mdata)^2))
  
}
library(caret)
library(Metrics)
set.seed(42)
train_index <- createDataPartition(data_heating_season$COP,p=0.70,list=FALSE)

train_data <- data_heating_season[train_index,]
test_data <- data_heating_season[-train_index,]

mycontrol <- trainControl(method = "cv",
                          number = 10,
                          verboseIter = T)

######Linear regression
set.seed(42)
mlr0 <- train(COP ~ outtemp, 
              data = train_data,  
              method = "lm",
              trControl = mycontrol)
summary(mlr0)
mlr0
rmse(test_data$COP,predict(mlr0, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(mlr0, test_data))


######Multi Linear regression
set.seed(42)
mlr1 <- train(COP ~ outtemp + flowtemp, 
              data = train_data,  
              method = "lm",
              trControl = mycontrol)
summary(mlr1)
mlr1
car::vif(mlr1$finalModel)
rmse(test_data$COP,predict(mlr1, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(mlr1, test_data))


#House 6 zone temp are the same dont include in models
set.seed(42)
mlr2 <- train(COP ~ ., 
              data = train_data[2:13],  
              method = "lm",
              trControl = mycontrol)
mlr2
summary(mlr2)
rmse(test_data$COP,predict(mlr2, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(mlr2, test_data))


#Backward selection MLR 
#Code needs to be change for each house
#Start with all variables and drop the stat insign variable rerun and drop the nest stat insign.  repeat till all variables stat sign. 
#Next check vif, IF variable is above 5 drop and rerun.
set.seed(42)
mlr3 <- train(COP ~ flowtemp + ASHP_on +
                steady_state + zone1temp +zone2temp  +
                dewpt  + rhum, 
              data = train_data,  
              method = "lm",  
              trControl = mycontrol)  
summary(mlr3)
mlr3
rmse(test_data$COP,predict(mlr3, test_data))
Rsqu(test_data$COP,predict(mlr3, test_data))
car::vif(mlr3$finalModel)

#GAM
set.seed(42)
gam_caret_10 <- train(
  COP ~ flowtemp + steady_state + outtemp +dewpt,
  data = train_data,
  method = "gam",
  trControl = mycontrol,
  importance = "impurity")
summary(gam_caret_10$finalModel)
gam_caret_10
concurvity(gam_caret_10$finalModel, full = FALSE)
rmse(test_data$COP,predict(gam_caret_10$finalModel, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(gam_caret_10, test_data[,c(2:13)]))

#RF
set.seed(42)
RF <- train(
  COP ~outtemp + flowtemp + ASHP_on +
    steady_state + zone1temp +
     dewpt +Rain + rhum + msl+wdsp,
  data = train_data,
  method = "ranger",
  trControl = mycontrol,
  importance = "impurity",metric='RMSE')
summary(RF)
RF
rmse(test_data$COP,predict(RF, test_data))
Rsqu(test_data$COP,predict(RF, test_data))


#RF2 change to reflect features selected by backward feature selection in MLR. 
set.seed(42)
RF2 <- train(
  COP ~ outtemp+ flowtemp+steady_state+rhum+ msl+ wdsp,
  data = train_data,
  method = "ranger",
  trControl = mycontrol,
  importance = "impurity",metric='RMSE')
summary(RF2 )
RF2

rmse(test_data$COP,predict(RF2, test_data))
Rsqu(test_data$COP,predict(RF2, test_data))


#########################
####SIM data
mysamp <- function(n, m, s, lwr, upr, rounding) {
  samp <- round(rnorm(n, m, s), rounding)
  samp[samp < lwr] <- lwr
  samp[samp > upr] <- upr
  samp
}

summary(data_heating_season$flowtemp)
sd(data_heating_season$flowtemp)

summary(data_heating_season$outtemp)
sd(data_heating_season$outtemp)

summary(data_heating_season$ASHP_on)
sd(data_heating_season$ASHP_on)

summary(data_heating_season$steady_state)
sd(data_heating_season$steady_state)

summary(data_heating_season$zone1temp)
sd(data_heating_season$zone1temp)

summary(data_heating_season$zone2temp)
sd(data_heating_season$zone2temp)

summary(data_heating_season$Rain)
sd(data_heating_season$Rain)

summary(data_heating_season$dewpt)
sd(data_heating_season$dewpt)

summary(data_heating_season$rhum)
sd(data_heating_season$rhum)

summary(data_heating_season$wdsp)
sd(data_heating_season$wdsp)

summary(data_heating_season$msl)
sd(data_heating_season$msl)
N  <- 100000
set.seed(42)
df  <- data.frame(id = 1:N, flowtemp = mysamp(n=N, m=   39.49   , s= 5.545327, lwr=   13.97    , upr= 53.38, rounding=0),
                  
                  outtemp =  mysamp(n=N, m=   7.008   , s=3.29742, lwr= -2.967 , upr=  17.200, rounding=0),
                  
                  ASHP_on = mysamp(n=N, m=50.54   , s=14.24474, lwr=  1.00  , upr=60, rounding=1), 
                  
                  steady_state = mysamp(n=N, m=  43.16, s=16.44839, lwr=0, upr=60, rounding=1),
                  
                  zone1temp = mysamp(n=N, m= 19.51 , s= 0.5686839, lwr= 17 , upr=21.78 , rounding=1),
                  
                  zone2temp = mysamp(n=N, m=18.76 , s= 0.677345, lwr= 16.87, upr=  20.78 , rounding=1),
                  
                  Rain = mysamp(n=N, m= 0.1307 , s=0.4484678, lwr=0, upr= 5.7000 , rounding=1),
                  
                  dewpt = mysamp(n=N, m=   4.613  , s=3.170962, lwr= -5.70, upr= 14.60, rounding=1),
                  
                  rhum = mysamp(n=N, m=  87.56 , s=    8.247817, lwr= 50 , upr=99, rounding=1),
                 
                   wdsp = mysamp(n=N, m=  9.013  , s= 4.715661, lwr=0, upr=28, rounding=1),
                  
                  msl = mysamp(n=N, m= 1008.4  , s= 14.44947, lwr= 971.4 , upr=  1037.4, rounding=1))

df$COP_RF <- predict(RF, df)
df$COP_RF <- round(df$COP_RF, 2)
summary(df)
#Start here
man_spec_7 <- df %>% filter(df$flowtemp ==35 & df$outtemp == 7)
summary(man_spec_7[,c(13)])
man_spec_2 <- df %>% filter(df$flowtemp ==35 & df$outtemp == 2)
summary(man_spec_2[,c(13)])
# For each House update, ie. shape, locationscale,rate, etc
library(fitdistrplus)
descdist(man_spec_2$COP_RF)
#GAMMA
fg <- fitdist(man_spec_2$COP_RF, "gamma")
summary(fg)
#Logistic
fl <- fitdist(man_spec_2$COP_RF, "logis")
summary(fl)
#Log norm
fln <- fitdist(man_spec_2$COP_RF, "lnorm")
summary(fln)
#norm
fn <- fitdist(man_spec_2$COP_RF, "norm")
summary(fn)
#exponential
fe <- fitdist(man_spec_2$COP_RF, "exp")
summary(fe)

plot.legend <- c( "Gamma", "Logistic", "lognormal", "norm","exp")
gofstat(list(fg, fl, fln, fn,fe),  fitnames = c("Gamma", "Logistic", "lognormal", "norm","exp") )
#
quantile(man_spec_2$COP)
#Change x for HP size
#manuf cop 2.95 at 2 degrees for 8.5kw
#manuf cop 3.34 at 2 degrees for 11kw
x <- 3.34
#3.34
pgamma(x,shape =  369.7978  , rate = 116.4271 )
plogis(x, location =  3.281367  , scale =  0.251578)
pnorm(x,mean = 2.9635354, sd =  0.4153721 )
plnorm(x, meanlog =  0.97791969,sdlog =  0.07908934)

#7 Degrees
descdist(man_spec_7$COP_RF)
#Normal
fn <- fitdist(man_spec_7$COP_RF, "norm")
summary(fn)
#GAMMA
fg <- fitdist(man_spec_7$COP_RF, "gamma")
summary(fg)
#Log norm
fln <- fitdist(man_spec_7$COP_RF, "lnorm")
summary(fln)
#LOGISTIC
fl <- fitdist(man_spec_7$COP_RF, "logis")
summary(fl)
#exponential
fe <- fitdist(man_spec_7$COP_RF, "exp")
summary(fe)

plot.legend <- c( "Normal", "Gamma", "lognormal", "logis", "exp")
denscomp(list(fn, fg, fln), xlab = "COP", legendtext = plot.legend) 
qqcomp(list(fn, fg, fln), legendtext = plot.legend)
cdfcomp(list(fn, fg, fln), xlab = "COP",legendtext = plot.legend)
ppcomp(list(fn, fg, fln), legendtext = plot.legend)
gofstat(list(fn, fg, fln,fl,fe),  fitnames = c( "Normal", "Gamma", "lognormal", "logistic", "exp") )


