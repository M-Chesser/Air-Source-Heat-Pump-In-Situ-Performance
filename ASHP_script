data$steady_state[data$OutsideTemperature >= 21 & data$FlowTemperature >= 55 & data$EnergyConsumed >= 27 & data$on == 1] <- 1 
data$steady_state_not <- 0
data$steady_state_not[data$on == 1 & data$steady_state == 0] <- 1 


data_full_dates <- seq.POSIXt(min(data$TimeStamp), max(data$TimeStamp), by = "min")
data_full_dates <- data.frame(Date = data_full_dates)
data_complete <- merge.data.frame(data_full_dates, data, by.x = "Date", by.y = "TimeStamp", all.x = TRUE)
data <- data_complete
rm(data_complete, data_full_dates)

statsNA(data$EnergyConsumed)


data$Datetime2 <- droplevels(cut(data$Date, breaks = "hour"))
#na.action = na.pass, corrects for when there is only several minutes in an hour resulting in over/under estimates 
#Variables that are Averaged
#Park et al (2018) intensive variables such as temperature and flow rate should be averaged. 
data_avg <- aggregate(cbind(RoomTemperatureZone1, 
                            RoomTemperatureZone2, 
                            FlowTemperature, OutsideTemperature) ~ Datetime2, 
                      data=data, FUN=mean, na.action = na.pass)     
#Variables that are summed
#Park et al (2018) extensive variables such as energy consumption should be summed. 
data_sum <- aggregate(cbind( 
  EnergyConsumed, 
  EnergyProduced,on,steady_state) ~ Datetime2, 
  data=data, FUN=sum, na.action = na.pass)

data_2 <- merge(data_avg, data_sum)
rm(data_avg,data_sum)

data_2$EnergyConsumed  <- na_seasplit(data_2$EnergyConsumed, find_frequency=TRUE )
data_2$EnergyProduced <- na_seasplit(data_2$EnergyProduced, find_frequency=TRUE )
data_2$RoomTemperatureZone1 <- na_seasplit(data_2$RoomTemperatureZone1, find_frequency=TRUE )
data_2$RoomTemperatureZone2 <- na_seasplit(data_2$RoomTemperatureZone2, find_frequency=TRUE )
data_2$FlowTemperature <- na_seasplit(data_2$FlowTemperature, find_frequency=TRUE)
data_2$OutsideTemperature <- na_seasplit(data_2$OutsideTemperature, find_frequency = TRUE)
data_2$on <- na_seasplit(data_2$on, find_frequency=TRUE)
data_2$steady_state <- na_seasplit(data_2$steady_state, find_frequency=TRUE)


data_2$EnergyConsumed  <- as.numeric(data_2$EnergyConsumed) 
data_2$EnergyProduced <- as.numeric(data_2$EnergyProduced)
data_2$RoomTemperatureZone1 <- as.numeric(data_2$RoomTemperatureZone1)
data_2$RoomTemperatureZone2 <- as.numeric(data_2$RoomTemperatureZone2)
data_2$FlowTemperature <- as.numeric(data_2$FlowTemperature)
data_2$OutsideTemperature <- as.numeric(data_2$OutsideTemperature)
data_2$on <- as.numeric(data_2$on)
data_2$steady_state <- as.numeric(data_2$steady_state)


data_2$Date <- strptime(as.character(data_2$Datetime2),format="%Y-%m-%d %H:%M:%S")
data_2$Date <- as.POSIXct(data_2$Date,tz = "UTC")
data_2$COP <- data_2$EnergyProduced/data_2$EnergyConsumed

#Heating seasons for houses 
#House6,7,8,11,12
data_heating_season <- data_2 %>% filter(Date > "2019-09-30 00:00:00" & Date < "2020-04-13 01:00:00")

#house 9 ,10, 4
data_heating_season <- data_2 %>% filter(Date > "2018-09-01 00:00:00" & Date < "2019-02-25 00:00:00")

#house 1,2,3
data_heating_season <- data_2 %>% filter(Date > "2019-01-13 23:00:00" & Date < "2020-01-06 00:00:00")
data_heating_season <- data_heating_season[-c(2185:6216),]

#change dates to match houses
setwd()
weatherstation <- read.table(file = "hly1475.csv", header = TRUE, sep = ",")
weatherstation$date<- dmy_hm(weatherstation$date)
weatherstation$date <- strptime(as.character(weatherstation$date),format="%Y-%m-%d %H:%M:%S")
weatherstation$date <- as.POSIXct(weatherstation$date,tz = "UTC")
weatherstation <- weatherstation %>% select(date,Rain,wetb,	
                                            dewpt,	              
                                            vappr,		
                                            rhum,		
                                            msl,	
                                            wdsp) %>% filter(date > "2019-09-30 00:00:00" & date < "2020-04-13 01:00:00") #change to correspond with house
weatherstation <- weatherstation[-c(2185:6216),]#only for houses 1,2,3

data_heating_season <- cbind(data_heating_season, Rain = weatherstation$Rain)
data_heating_season <- cbind(data_heating_season, dewpt = weatherstation$dewpt)
data_heating_season <- cbind(data_heating_season, vappr = weatherstation$vappr)
data_heating_season <- cbind(data_heating_season, rhum = weatherstation$rhum)
data_heating_season <- cbind(data_heating_season, msl	 = weatherstation$msl)
data_heating_season <- cbind(data_heating_season, wdsp	 = weatherstation$wdsp)
rm(weatherstation)

data_heating_season <- data_heating_season %>% filter(data_heating_season$on >0 )
data_heating_season <- data_heating_season %>% filter(data_heating_season$COP >0 )
data_heating_season <-  data_heating_season[!(data_heating_season$on > 45 & data_heating_season$COP < 1 ),]
summary(data_heating_season$COP)

data_heating_season <- data_heating_season %>% rename(flowtemp = FlowTemperature)
data_heating_season <- data_heating_season %>% rename(ASHP_on = on)
data_heating_season <- data_heating_season %>% rename(outtemp = OutsideTemperature)
data_heating_season <- data_heating_season %>% rename(zone1temp = RoomTemperatureZone1)
data_heating_season <- data_heating_season %>% rename(zone2temp = RoomTemperatureZone2)

data_heating_season <- data_heating_season %>% select( Date,COP,flowtemp,ASHP_on, 
                                                       steady_state, zone1temp,
                                                       zone2temp, Rain,outtemp, dewpt,rhum,msl,wdsp )

#Outlier dectation LOF
library(DMwR2)
library(lattice)
library(grid)

LOF <- data_heating_season  %>% select(COP,flowtemp,ASHP_on, 
                           steady_state, zone1temp,
                           zone2temp, Rain,outtemp, dewpt,rhum,msl,wdsp)

#Standardization ensures that features with large mean or 
#variance do not disproportionately influence the score
set.seed(42)
LOF <- scale(LOF)
set.seed(42)
outlier.scores <- lofactor(LOF, k=5)
outliers <- order(outlier.scores, decreasing=T)[1:100]
print(outliers)

