table(data$OperatingMode)
data$on <- NA
data$on[(data$OperatingMode == "Hot Water" | data$OperatingMode == "Legionella Prevention" | data$OperatingMode == "Heating" )] = 1 
data$on <- na_replace(data$on, fill = 0)
data$steady_state <- 0
data$steady_state[data$OutsideTemperature < -7  & data$FlowTemperature < 25 & data$EnergyConsumed >= 16 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -6  & data$FlowTemperature < 25 & data$EnergyConsumed >= 16 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -5  & data$FlowTemperature < 25 & data$EnergyConsumed >= 15 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -4  & data$FlowTemperature < 25 & data$EnergyConsumed >= 15 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -3  & data$FlowTemperature < 25 & data$EnergyConsumed >= 14 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -2  & data$FlowTemperature < 25 & data$EnergyConsumed >= 14 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -1  & data$FlowTemperature < 25 & data$EnergyConsumed >= 13 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 0  & data$FlowTemperature < 25 & data$EnergyConsumed >= 13 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 1  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 2  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 3  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 4  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1
data$steady_state[data$OutsideTemperature < 5  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1
data$steady_state[data$OutsideTemperature < 6  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1 
