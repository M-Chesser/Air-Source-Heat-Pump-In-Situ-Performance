##MODELS
#splitting the data set test and train
library(caret)
library(Metrics)
set.seed(42)
train_index <- createDataPartition(data_on$COP,p=0.70,list=FALSE)

train_data <- data_on[train_index,]
test_data <- data_on[-train_index,]

mycontrol <- trainControl(method = "cv",
                          number = 10,
                          verboseIter = T)
#Rsq
Rsqu <- function(y, yhat) {
  
  mdata <- mean(y)
  
  1 - sum((y-yhat)^2)/(sum((y-mdata)^2))
  
}


#First model is the bench mark model COP v outside temp.
######Linear regression
set.seed(42)
mlr0 <- train(COP ~ outtemp, 
              data = train_data,  
              method = "lm",
              preProc = c("zv", "nzv"),
              trControl = mycontrol)

summary(mlr0)
mlr0
rmse(test_data$COP,predict(mlr0, test_data))
Rsqu(test_data$COP,predict(mlr0, test_data))
png('mlr0_diagnostic.png',width = 8, height = 6, units = 'in', res = 300)
par(mfrow=c(2,2)) # Change the panel layout to 2 x 2
plot(mlr0$finalModel)
dev.off()

######Multi Linear regression
#MLR1
set.seed(42)
mlr1 <- train(COP ~ outtemp + flowtemp, 
              data = train_data,  
              method = "lm", 
              preProc = c("zv", "nzv"),
              trControl = mycontrol)
summary(mlr1)
mlr1
car::vif(mlr1$finalModel)
rmse(test_data$COP,predict(mlr1, test_data))
Rsqu(test_data$COP,predict(mlr1, test_data))
png('mlr1_diagnostic.png',width = 8, height = 6, units = 'in', res = 300)
par(mfrow=c(2,2)) # Change the panel layout to 2 x 2
plot(mlr1$finalModel)
dev.off()

#MLR2 2
####MLR with all variables
set.seed(42)
mlr2 <- train(COP ~ ., 
              data = train_data[2:13],  
              method = "lm",  
              trControl = mycontrol)
summary(mlr2)
mlr2
car::vif(mlr2$finalModel)
rmse(test_data$COP,predict(mlr2, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(mlr2, test_data))
png('mlr2_diagnostic.png',width = 8, height = 6, units = 'in', res = 300)
par(mfrow=c(2,2)) # Change the panel layout to 2 x 2
plot(mlr2$finalModel)
dev.off()

#Next two models used the in built stepwise selection method which based model selection on AIC
#Because I dont have the option to run this method based on RMSE and to avoid confusion in the paper
#Skip to line 1455
#step regression model 
set.seed(42)
mlrss1 <- train(COP ~ ., 
                data = train_data[,c(2:13)],  
                method = "lmStepAIC",  
                trControl = mycontrol)
mlrss1
summary(mlrss1$finalModel)
car::vif(mlrss1$finalModel)
rmse(test_data$COP,predict(mlrss1, test_data[,c(2:13)]))
Rsqu(test_data$COP,predict(mlrss1, test_data))
###Linear Regression with Stepwise Selection
#ASHP on left out 
set.seed(42)
mlrss2 <- train(COP ~ ., 
                data = train_data[,c(2,3,5:13)],  
                method = "lmStepAIC",  
                trControl = mycontrol)
summary(mlrss2$finalModel)
mlrss2
car::vif(mlrss2$finalModel)
plot(mlrss2$finalModel)
rmse(test_data$COP,predict(mlrss2, test_data))
Rsqu(test_data$COP,predict(mlrss2, test_data))
test_data$COP_mlrss2 <- predict(mlrss2, test_data)
png('mlrss2_diagnostic.png',width = 8, height = 6, units = 'in', res = 300)
par(mfrow=c(2,2)) # Change the panel layout to 2 x 2
plot(mlrss2$finalModel)
dev.off()

