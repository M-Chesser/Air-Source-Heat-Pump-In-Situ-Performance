data_heating_season <- data_heating_season[-c(455,321,744,3594,1282, 454,2023,2623,1222,1034,3391, 308,3595,3396,3087,3382,1071,2838,1267,3607,3598,2725,1261,1142,3604,2837,2492,56,1506,1880,1259, 421,2449,2720, 335,1529,39,1072, 261,2859,
