#Figures for COPvflowtemp  & outtemp
cop_v_flow <- ggplot(data_on, aes(x = flowtemp , y = COP)) + 
  geom_point(shape = 20,size = 0.01) +
  labs( x = "Outflow temperature (C)") + 
  theme(axis.text = element_text(size = 12 ), 
        axis.title.x = element_text(face = "bold", size = 12),
        axis.title.y = element_text(face = "bold", size = 12))
cop_v_outside <- ggplot(data_on, aes(x = outtemp , y = COP)) + 
  geom_point(shape = 20,size = 0.01) + 
  labs( x = "Outside temperature (C)") + 
  theme(axis.text = element_text(size = 12 ), 
        axis.title.x = element_text(face = "bold" , size = 12),
        axis.title.y = element_text(face = "bold", size = 12))
library(ggpubr)
D_plots <- ggarrange( cop_v_flow,cop_v_outside, 
                      labels = c("A", "B"), font.label = list(size = 4, face = "bold"),
                      ncol = 2, nrow = 1)
D_plots
ggsave("Descriptive Plots.png", D_plots, 
       width = 15.6, 
       height = 7.5, 
       dpi = 250, units = "cm")


#3D PLOT
library(plotly)
zoomed_in <- data_on %>% filter(data_on$flowtemp >34.9 & data_on$flowtemp < 36 & data_on$outtemp < 7.01 & data_on$outtemp > 1.9)

fig <- plot_ly() %>% 
  add_trace(data = data_on, x=data_on$outtemp, 
            y=data_on$flowtemp, z=data_on$COP, 
            type="mesh3d", intensity = data_on$COP,colorbar=list(
              title='COP',titlefont = list(size = 24),tickfont = list(size = 16)),colorscale = list(c(0,'green'), c(0.33,'yellow'), c(0.66, 'orange'),  c(1, 'red')))
fig <- fig %>% layout(
  scene = list(
    xaxis = list(title = "outtemp (C)",titlefont = list(size = 24),tickfont = list(size = 16)),
    yaxis = list(title = "flowtemp (C)",titlefont = list(size = 24),tickfont = list(size = 16)),
    zaxis = list(title = "COP ", titlefont = list(size = 24),tickfont = list(size = 16))
  ))
fig

fig_2 <- plot_ly() %>% 
  add_trace(data = zoomed_in, x=zoomed_in$outtemp, 
            y=zoomed_in$flowtemp , z=zoomed_in$COP, 
            type="mesh3d", intensity = data_on$COP,colorbar=list(
              title='COP',titlefont = list(size = 24),tickfont = list(size = 16)),colorscale = list(c(0,'green'), c(0.33,'yellow'), c(0.66, 'orange'), c(1, 'red')))
fig_2 <- fig_2 %>% 
  layout(scene = list(
    xaxis = list(title ="outtemp (C)" ,titlefont = list(size = 20),tickfont = list(size = 15), ticksuffix = ""),
    yaxis = list(title = "flowtemp (C)",titlefont = list(size = 20),tickfont = list(size = 15), ticksuffix = ""),
    zaxis = list(title = "COP ", titlefont = list(size = 20),tickfont = list(size = 15))
  )) 
fig_2


