# Line 10 to 1340 is importing data and cleaning
# line 1343 to load data ready for modelling
#Modelling begins at 1305 where you can load in the clean dataset read
#Simulated data for modeling 1800. line 1947 to load simulated dataset


library(mgcv)
library(car)
library(lubridate) 
library(e1071)
library(imputeTS)
library(dplyr)
library(ggpmisc)
library(ggplot2)
library(readxl)

#HOUSE 5 was chosen as it is the must complete in terms of data.
setwd()
data <- read_excel("House_5_1.xlsx")
head(data)
#Date                     RoomTemperatureZone1 RoomTemperatureZone2 OutsideTemperature FlowTemperature EnergyConsumed EnergyProduced OperatingMode 
#1 2019-05-26 13:13:00                   23                 23.5                 18            45.0             27             45     Hot Water  
#2 2019-05-26 13:14:00                   23                 23.5                 18            46.5             35             66     Hot Water  
#3 2019-05-26 13:15:00                   23                 23.5                 18            47.5             38             87     Hot Water  


#Data set is at the 1 minute level and contains eight variables.




#the variable operating mode varied from house to house, not all houses recorded when the ASHP was in legionella prevention.
#The ASHP Operating mode may be listed as "stop" or "power off" but the energy produced variable may sometimes record that energy was produced.   
#There is a lag between the two but the size of the energy produced recorded is very small. 
#I decided a better way was to create a variable with the value of 1 when it was in heat producing mode and 0 when not.
#
table(data$OperatingMode)
data$on <- NA
data$on[(data$OperatingMode == "Hot Water" | data$OperatingMode == "Legionella Prevention" | data$OperatingMode == "Heating" )] = 1 
data$on <- na_replace(data$on, fill = 0)
#The following code relates to when the compressor is in a steady state. Cycling of the compressor from on to off to on again decreases perfromance.
#POR from  supplied operating conditions that results in steady state. See excel file Thresholds

data$steady_state <- 0
#The first line says that if the outside temp is less than  -7 and the flow temp is less than 25 and energy consumed is greater than or equal to 
# 25 that the compressor is in a staedy state for that minute. 
data$steady_state[data$OutsideTemperature < -7  & data$FlowTemperature < 25 & data$EnergyConsumed >= 16 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -6  & data$FlowTemperature < 25 & data$EnergyConsumed >= 16 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -5  & data$FlowTemperature < 25 & data$EnergyConsumed >= 15 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -4  & data$FlowTemperature < 25 & data$EnergyConsumed >= 15 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -3  & data$FlowTemperature < 25 & data$EnergyConsumed >= 14 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -2  & data$FlowTemperature < 25 & data$EnergyConsumed >= 14 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < -1  & data$FlowTemperature < 25 & data$EnergyConsumed >= 13 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 0  & data$FlowTemperature < 25 & data$EnergyConsumed >= 13 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 1  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 2  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 3  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 4  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1
data$steady_state[data$OutsideTemperature < 5  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1
data$steady_state[data$OutsideTemperature < 6  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 7  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1
data$steady_state[data$OutsideTemperature < 8  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1
data$steady_state[data$OutsideTemperature < 9  & data$FlowTemperature < 25 & data$EnergyConsumed >= 12 & data$on == 1] <- 1 
data$steady_state[data$OutsideTemperature < 11 & data$FlowTemperature < 25 & data$EnergyConsumed >= 11 & data$on == 1] <- 1
data$steady_state[data$OutsideTemperature < 11 & data$FlowTemperature < 25 & data$EnergyConsumed >= 11 & data$on == 1] <- 1
