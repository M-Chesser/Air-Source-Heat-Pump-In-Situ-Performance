###GAM
#Started with all variables, checking concurvity and post-diagnostics after each model to get the best one. 
set.seed(42)
gam_caret <- train(
  COP ~ flowtemp + steady_state + zone1temp +
    zone2temp + outtemp + Rain + dewpt + rhum + msl +wdsp,
  data = train_data,
  method = "gam",
  trControl = mycontrol,
  importance = "impurity")
#summary(gam_caret)
plot(gam_caret$finalModel, pages = 1, shift = coef(gam_caret)[1])
plot(gam_caret$finalModel, )
concurvity(gam_caret$finalModel, full = TRUE)
#              para     s(wdsp)         s(rhum)   s(steady_state)  s(dewpt)  s(zone2temp) s(zone1temp)    s(msl) s(outtemp) s(flowtemp)
#worst    5.822714e-17 0.4473126     0.6800950       0.7139780     0.9140385   0.47679596    0.6147444 0.3687114  0.9185691   0.5551005
concurvity(gam_caret$finalModel, full = FALSE)
summary(gam_caret)
gam_caret$finalModel

gam_caret_0 <- train(
  COP ~ outtemp ,
  data = train_data,
  method = "gam",
  trControl = mycontrol,
  importance = "impurity")
gam_caret_0
summary(gam_caret_0$finalModel)
gam.check(gam_caret_0$finalModel)

set.seed(42)
gam_caret_1 <- train(
  COP ~ flowtemp + outtemp ,
  data = train_data,
  method = "gam",
  trControl = mycontrol,
  importance = "impurity")
gam_caret_1
summary(gam_caret_1$finalModel)
gam.check(gam_caret_1$finalModel)
concurvity(gam_caret_1$finalModel, full = TRUE)
concurvity(gam_caret_1$finalModel, full = FALSE)
plot(gam_caret_1$finalModel, page = 1, shift = coef(gam_caret_1))
plot.gam(gam_caret_1, shift = coef(gam_caret_1))
vis.gam(gam_caret_1$finalModel, view = c( "flowtemp", "outtemp"), ticktype = "detailed",  r = 10, 
        phi = 20, theta= 40)

